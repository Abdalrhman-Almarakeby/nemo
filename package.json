  "homepage": "https://github.com/z4nr34l/next-easy-middlewares#readme",
  "bugs": {
    "url": "https://github.com/z4nr34l/next-easy-middlewares/issues"
  },
  "repository": "https://github.com/z4nr34l/next-easy-middlewares",
  "license": "MIT",
  "author": "Z4NR34L <software@zanreal.net>",
  "scripts": {
    "build": "turbo run build",
    "changeset": "changeset",
    "clean": "turbo clean",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "lint": "turbo run lint",
    "prepare": "husky install",
    "release": "turbo build --filter='@rescale/nemo' && changeset publish",
    "version-packages": "changeset version"
  },
  "prettier": "@vercel/style-guide/prettier",
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@commitlint/config-pnpm-scopes": "^18.4.3",
    "@vercel/style-guide": "^5.1.0",
    "husky": "^8.0.3",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "turbo": "latest"
  },
  "packageManager": "pnpm@9.5.0",
  "publishConfig": {
    "provenance": true
  },
  "pnpm": {
    "overrides": {
      "eslint": "^8"
    }
  }
